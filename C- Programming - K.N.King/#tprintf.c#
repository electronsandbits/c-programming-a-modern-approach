/*
 * File: tprintf.c 
 * ----------------
 * This program illustrates the use of printf to print
 * integers and floating-point numbers in various formats.
 */


#include <stdio.h>


int main(void) {
   int i;
   float x;
 
   i = 40;
   x = 839.21f;

   printf("|%d|%5d|%-5d|%5.3d|\n",i, i, i, i);
   printf("|%10.3f|%10.3e|%5.3d|\n",i, i, i, i);



}
